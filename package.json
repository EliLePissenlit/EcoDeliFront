{
  "name": "@ecodeli/frontend",
  "version": "0.1.0",
  "description": "Ecodeli frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:types": "graphql-codegen --config codegen.ts --verbose && npm run lint:fix",
    "dev": "vite",
    "start": "serve -s dist",
    "build": "tsc && vite build",
    "build:deploy": "gcloud builds submit --config deploy.yaml",
    "build:deploy:staging": "gcloud builds submit --config deploy.staging.yaml",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "dev:host": "vite --host"
  },
